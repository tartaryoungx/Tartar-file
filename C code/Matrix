#include<stdio.h>
main(){
    int m[110][110],n[110][110];
    int a,b,c,d;
    int i,j;
    scanf("%d%d",&c,&d);


    for(a=1;a<=c;a++){

        for(b=1;b<=d;b++){
        scanf("%d",&m[a][b]);
        }
    }
    for(a=1;a<=c;a++){

        for(b=1;b<=d;b++){

            scanf("%d",&n[a][b]);
        }
    }
    for(a=1;a<=c;a++){

        for(b=1;b<=d;b++){

            printf("%d ",m[a][b]+n[a][b]);
        }
        printf("\n");
    }
    return 0;
}
